{
  "name": "boil-react-ts",
  "version": "0.0.0",
  "description": "My personal boilerplate for basic react typescript apps",
  "scripts": {
    "dev": "parcel src/index.html",
    "build": "parcel build src/index.html"
  },
  "author": "Harris Schneiderman",
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/parcel-bundler": "^1.12.1",
    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.8",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^7.3.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "jsondiffpatch": "^0.4.1"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "none"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,css,md,ts,tsx}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "last 3 Chrome versions"
  ]
}
